  "devDependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/jszip": "^3.4.1",
    "pug-cli": "^1.0.0-alpha6",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
