{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  },

  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": false,
    "no-bitwise": false,
    "prefer-for-of": false,
    "variable-name": false,
    "jsx-no-lambda": false
  }
}
